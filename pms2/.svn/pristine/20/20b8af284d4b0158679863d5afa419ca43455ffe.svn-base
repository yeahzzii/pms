package pms.a01_Yeji.a04_VO;

public class PrjFinance {
	private String prjno;
	private int prjfinance;
	public PrjFinance() {
		// TODO Auto-generated constructor stub
	}
	public PrjFinance(String prjno, int prjfinance) {
		this.prjno = prjno;
		this.prjfinance = prjfinance;
	}
	public String getPrjno() {
		return prjno;

package pms.a01_Yeji.a04_VO;

public class Important {
	private String empno;
	private String ename;
	private String eImportant;
	public Important() {
		// TODO Auto-generated constructor stub
	}
	public Important(String empno, String ename, String eImportant) {
		this.empno = empno;
		this.ename = ename;
		this.eImportant = eImportant;
	}
	public String getEmpno() {
		return empno;
	}
	public void setEmpno(String empno) {
		this.empno = empno;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public String geteImportant() {
		return eImportant;
	}
	public void seteImportant(String eImportant) {
		this.eImportant = eImportant;
	}

}

package pms.a01_Jongdae.a04_VO;

import java.util.Date;

public class RoadMap {
	private String rmNo;
	private String prjNo;
	private String rmName;
	private Date rmStartDate;
	private Date rmEndDate;
	private int rmCost;
	private String prjName;
	public RoadMap() {
		// TODO Auto-generated constructor stub
	}
	public RoadMap(String prjNo) {
		this.prjNo = prjNo;
	}
	public String getRmNo() {
		return rmNo;
	}
	public void setRmNo(String rmNo) {
		this.rmNo = rmNo;
	}
	public String getPrjNo() {
		return prjNo;
	}
	public void setPrjNo(String prjNo) {
		this.prjNo = prjNo;
	}
	public String getRmName() {
		return rmName;
	}
	public void setRmName(String rmName) {
		this.rmName = rmName;
	}
	public Date getRmStartDate() {
		return rmStartDate;
	}
	public void setRmStartDate(Date rmStartDate) {
		this.rmStartDate = rmStartDate;
	}
	public Date getRmEndDate() {
		return rmEndDate;
	}
	public void setRmEndDate(Date rmEndDate) {
		this.rmEndDate = rmEndDate;
	}
	public int getRmCost() {
		return rmCost;
	}
	public void setRmCost(int rmCost) {
		this.rmCost = rmCost;
	}
	public String getPrjName() {
		return prjName;
	}
	public void setPrjName(String prjName) {
		this.prjName = prjName;
	}
	
	
}

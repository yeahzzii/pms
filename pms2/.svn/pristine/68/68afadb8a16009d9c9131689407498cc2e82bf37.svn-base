package pms.a01_Yeji.a04_VO;

import java.util.Date;

import org.springframework.web.multipart.MultipartFile;

public class MeetingBoard {
	private int cnt;
	private int recordNum;
	private String recordTitle;
	private String reocrdContent;
	private String ename;
	private int empno;
	private int recordViewCnt;
	private Date recordRegdte;
	private Date recordUptdte;
	private MultipartFile report;
	private String mrFileName;
	private String mrFilePath;
	public MeetingBoard() {
		// TODO Auto-generated constructor stub
	}
	
	public MeetingBoard(int recordNum, String recordTitle, String reocrdContent, String ename, int empno,
			int recordViewCnt, Date recordRegdte, Date recordUptdte) {
		this.recordNum = recordNum;
		this.recordTitle = recordTitle;
		this.reocrdContent = reocrdContent;
		this.ename = ename;
		this.empno = empno;
		this.recordViewCnt = recordViewCnt;
		this.recordRegdte = recordRegdte;
		this.recordUptdte = recordUptdte;
	}

	public MeetingBoard(int cnt, int recordNum, String recordTitle, String reocrdContent, String ename, int empno,
			int recordViewCnt, Date recordRegdte, Date recordUptdte, MultipartFile report, String mrFileName,
			String mrFilePath) {
		this.cnt = cnt;
		this.recordNum = recordNum;
		this.recordTitle = recordTitle;
		this.reocrdContent = reocrdContent;
		this.ename = ename;
		this.empno = empno;
		this.recordViewCnt = recordViewCnt;
		this.recordRegdte = recordRegdte;
		this.recordUptdte = recordUptdte;
		this.report = report;
		this.mrFileName = mrFileName;
		this.mrFilePath = mrFilePath;
		
	}
	public int getCnt() {
		return cnt;
	}
	public void setCnt(int cnt) {
		this.cnt = cnt;
	}
	public int getRecordNum() {
		return recordNum;
	}
	public void setRecordNum(int recordNum) {
		this.recordNum = recordNum;
	}
	public String getRecordTitle() {
		return recordTitle;
	}
	public void setRecordTitle(String recordTitle) {
		this.recordTitle = recordTitle;
	}
	public String getReocrdContent() {
		return reocrdContent;
	}
	public void setReocrdContent(String reocrdContent) {
		this.reocrdContent = reocrdContent;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public int getRecordViewCnt() {
		return recordViewCnt;
	}
	public void setRecordViewCnt(int recordViewCnt) {
		this.recordViewCnt = recordViewCnt;
	}
	public Date getRecordRegdte() {
		return recordRegdte;
	}
	public void setRecordRegdte(Date recordRegdte) {
		this.recordRegdte = recordRegdte;
	}
	public Date getRecordUptdte() {
		return recordUptdte;
	}
	public void setRecordUptdte(Date recordUptdte) {
		this.recordUptdte = recordUptdte;
	}
	public MultipartFile getReport() {
		return report;
	}
	public void setReport(MultipartFile report) {
		this.report = report;
	}
	public String getMrFileName() {
		return mrFileName;
	}
	public void setMrFileName(String mrFileName) {
		this.mrFileName = mrFileName;
	}
	public String getMrFilePath() {
		return mrFilePath;
	}
	public void setMrFilePath(String mrFilePath) {
		this.mrFilePath = mrFilePath;
	}

	public int getEmpno() {
		return empno;
	}
	public void setEmpno(int empno) {
		this.empno = empno;
	}
	
	



}

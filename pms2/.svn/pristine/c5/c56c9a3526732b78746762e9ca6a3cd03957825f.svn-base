package pms.a01_Yeji.a04_VO;

public class RiskDeg {
	private String riskdeg;
	private int riskCnt;
	public RiskDeg() {
		// TODO Auto-generated constructor stub
	}
	public RiskDeg(String riskdeg, int riskCnt) {
		this.riskdeg = riskdeg;
		this.riskCnt = riskCnt;
	}
	public String getRiskdeg() {
		return riskdeg;
	}
	public void setRiskdeg(String riskdeg) {
		this.riskdeg = riskdeg;
	}
	public int getRiskCnt() {
		return riskCnt;
	}
	public void setRiskCnt(int riskCnt) {
		this.riskCnt = riskCnt;
	}
	

}

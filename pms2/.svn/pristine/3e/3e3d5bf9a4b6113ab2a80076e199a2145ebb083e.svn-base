package pms.a01_Sechan.a04_VO;
// pms.a01_Sechan.a04_VO.Reviewplan
import java.util.Date;

public class Reviewplan {
	/*
	 rplanNo	VARCHAR2(50) PRIMARY KEY,
	rplanName	VARCHAR2(50),
	rplanStep	VARCHAR2(20),
	clientRequire	VARCHAR2(200),
	specialNote	VARCHAR2(100),
	rplanNote	VARCHAR2(100),
	requestDate	DATE,
	approvalDate	DATE,
	rplanStatus	VARCHAR2(50),
	prjNo	VARCHAR2(50) REFERENCES project(prjNo)
	 */
	private String rplanNo;
	private String rplanName;
	private String rplanStep;
	private String clientRequire;
	private String specialNote;
	private String rplanNote;
	private Date requestDate;
	private Date approvalDate;
	private String rplanStatus;
	private String prjNo;
	private String prjName;
	private String prjPMNo;
	private String pmname;
	public Reviewplan() {
		// TODO Auto-generated constructor stub
	}
	
	public String getPmname() {
		return pmname;
	}

	public void setPmname(String pmname) {
		this.pmname = pmname;
	}

	public String getRplanNo() {
		return rplanNo;
	}
	public void setRplanNo(String rplanNo) {
		this.rplanNo = rplanNo;
	}
	public String getRplanName() {
		return rplanName;
	}
	public void setRplanName(String rplanName) {
		this.rplanName = rplanName;
	}
	public String getRplanStep() {
		return rplanStep;
	}
	public void setRplanStep(String rplanStep) {
		this.rplanStep = rplanStep;
	}
	public String getClientRequire() {
		return clientRequire;
	}
	public void setClientRequire(String clientRequire) {
		this.clientRequire = clientRequire;
	}
	public String getSpecialNote() {
		return specialNote;
	}
	public void setSpecialNote(String specialNote) {
		this.specialNote = specialNote;
	}
	public String getRplanNote() {
		return rplanNote;
	}
	public void setRplanNote(String rplanNote) {
		this.rplanNote = rplanNote;
	}
	public Date getRequestDate() {
		return requestDate;
	}
	public void setRequestDate(Date requestDate) {
		this.requestDate = requestDate;
	}
	public Date getApprovalDate() {
		return approvalDate;
	}
	public void setApprovalDate(Date approvalDate) {
		this.approvalDate = approvalDate;
	}
	public String getRplanStatus() {
		return rplanStatus;
	}
	public void setRplanStatus(String rplanStatus) {
		this.rplanStatus = rplanStatus;
	}
	public String getPrjNo() {
		return prjNo;
	}
	public void setPrjNo(String prjNo) {
		this.prjNo = prjNo;
	}
	public String getPrjName() {
		return prjName;
	}
	public void setPrjName(String prjName) {
		this.prjName = prjName;
	}
	public String getPrjPMNo() {
		return prjPMNo;
	}
	public void setPrjPMNo(String prjPMNo) {
		this.prjPMNo = prjPMNo;
	}
	
	
	
	
}

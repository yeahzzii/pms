package pms.a01_Yeji.a04_VO;

public class MeetingBoardSch {
	// 페이징 처리 및 검색을 위한 vo
	// 1. 검색
	private String recordTitle;
	private String ename;
	// 2. 페이징 처리를 위한 속성
	private int recordCount; // 총데이터 건수
	private int recordPageSize; // 한번에 보여줄 페이ㅣㅈ 크기
	private int recordPageCount; // 총 페이지 수 
	private int recordCurPage; // 클릭한 현재 페이지 번호
	private int recordStart; // 시작 번호(현재페이지)
	private int recordEnd; // 마지막 번호(현재페이지)
	// 3. 하단에 block 속성
	private int recordBlockSize; // 한번에 보여줄 block의 크기
	private int recordStartBlock; // block의 시작번호
	private int recordEndBlock; // block의 마지막번호
	public MeetingBoardSch() {
		// TODO Auto-generated constructor stub
	}
	public MeetingBoardSch(String recordTitle, String ename, int recordCount, int recordPageSize, int recordPageCount,
			int recordCurPage, int recordStart, int recordEnd, int recordBlockSize, int recordStartBlock,
			int recordEndBlock) {
		this.recordTitle = recordTitle;
		this.ename = ename;
		this.recordCount = recordCount;
		this.recordPageSize = recordPageSize;
		this.recordPageCount = recordPageCount;
		this.recordCurPage = recordCurPage;
		this.recordStart = recordStart;
		this.recordEnd = recordEnd;
		this.recordBlockSize = recordBlockSize;
		this.recordStartBlock = recordStartBlock;
		this.recordEndBlock = recordEndBlock;
	}
	public String getRecordTitle() {
		return recordTitle;
	}
	public void setRecordTitle(String recordTitle) {
		this.recordTitle = recordTitle;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public int getRecordCount() {
		return recordCount;
	}
	public void setRecordCount(int recordCount) {
		this.recordCount = recordCount;
	}
	public int getRecordPageSize() {
		return recordPageSize;
	}
	public void setRecordPageSize(int recordPageSize) {
		this.recordPageSize = recordPageSize;
	}
	public int getRecordPageCount() {
		return recordPageCount;
	}
	public void setRecordPageCount(int recordPageCount) {
		this.recordPageCount = recordPageCount;
	}
	public int getRecordCurPage() {
		return recordCurPage;
	}
	public void setRecordCurPage(int recordCurPage) {
		this.recordCurPage = recordCurPage;
	}
	public int getRecordStart() {
		return recordStart;
	}
	public void setRecordStart(int recordStart) {
		this.recordStart = recordStart;
	}
	public int getRecordEnd() {
		return recordEnd;
	}
	public void setRecordEnd(int recordEnd) {
		this.recordEnd = recordEnd;
	}
	public int getRecordBlockSize() {
		return recordBlockSize;
	}
	public void setRecordBlockSize(int recordBlockSize) {
		this.recordBlockSize = recordBlockSize;
	}
	public int getRecordStartBlock() {
		return recordStartBlock;
	}
	public void setRecordStartBlock(int recordStartBlock) {
		this.recordStartBlock = recordStartBlock;
	}
	public int getRecordEndBlock() {
		return recordEndBlock;
	}
	public void setRecordEndBlock(int recordEndBlock) {
		this.recordEndBlock = recordEndBlock;
	}
	
	
}
